                <pfx:OracleID>
                    <xsl:value-of select="$JDBC-Query/resultSet/Record[1]/ORACLEID"/>
                </pfx:OracleID>
                <pfx:Bllability>
                    <xsl:value-of select="$JDBC-Query/resultSet/Record[1]/BILLABILITY"/>
                </pfx:Bllability>
                <pfx:Role>
                    <xsl:value-of select="$JDBC-Query/resultSet/Record[1]/ROLE"/>
                </pfx:Role>
                <pfx:Designation>
                    <xsl:value-of select="$JDBC-Query/resultSet/Record[1]/DESIGNATION"/>
                </pfx:Designation>
                <pfx:PID>
                    <xsl:value-of select="$JDBC-Query/resultSet/Record[1]/PID"/>
                </pfx:PID>
                <pfx:ProjectName>
                    <xsl:value-of select="$JDBC-Query/resultSet/Record[1]/PROJECTNAME"/>
                </pfx:ProjectName>
                <pfx:PIDStartDate>
                    <xsl:value-of select="substring-before($JDBC-Query/resultSet/Record[1]/PIDSTARTDATE, &quot;T&quot;)"/>
                </pfx:PIDStartDate>
                <pfx:PIDEndDate>
                    <xsl:value-of select="substring-before($JDBC-Query/resultSet/Record[1]/PIDENDDATE, &quot;T&quot;)"/>
                </pfx:PIDEndDate>
            </pfx:ProjectAssignment>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Map-Data-1">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>394</pd:x>
        <pd:y>167</pd:y>
        <config>
            <element ref="pfx2:Error"/>
        </config>
        <pd:inputBindings>
            <xsl:choose>
                <xsl:when test="exists($_error_JDBC-Query/ActivityErrors)">
                    <pfx2:Error>
                        <pfx2:ErrorStatus>
                            <xsl:value-of select="&quot;Error in Fetching Employee Project Assignment Details&quot;"/>
                        </pfx2:ErrorStatus>
                        <pfx2:ErrorCode>
                            <xsl:value-of select="$_error/ns:ErrorReport/MsgCode"/>
                        </pfx2:ErrorCode>
                    </pfx2:Error>
                </xsl:when>
                <xsl:otherwise>
                    <pfx2:Error>
                        <pfx2:ErrorStatus>
                            <xsl:value-of select="$_error/ns:ErrorReport/Msg"/>
                        </pfx2:ErrorStatus>
                        <pfx2:ErrorCode>
                            <xsl:value-of select="$_error/ns:ErrorReport/MsgCode"/>
                        </pfx2:ErrorCode>
                    </pfx2:Error>
                </xsl:otherwise>
            </xsl:choose>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Render JSON">
