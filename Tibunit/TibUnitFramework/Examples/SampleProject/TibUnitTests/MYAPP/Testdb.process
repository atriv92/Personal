<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:BW="java://com.tibco.pe.core.JavaCustomXPATHFunctions" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns2="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns1="http://www.tibco.com/pe/EngineTypes" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/schemas/BWUnitTest/Schema.xsd" xmlns:ns3="http://www.tibco.com/schemas/TibUnit/Schema.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <pd:name>TibUnitTests/MYAPP/Testdb.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="input" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="output" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="TibUnit">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="processFileName" type="xsd:string"/>
                                <xsd:element name="parentFolder" type="xsd:string"/>
                                <xsd:element name="completeProcessName" type="xsd:string"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>100</pd:startX>
    <pd:startY>59</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>462</pd:endX>
    <pd:endY>61</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1452005389236</pd:targetNamespace>
    <pd:activity name="AssertDB">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>347</pd:x>
        <pd:y>61</pd:y>
        <config>
            <processName>/TibUnit/Assert.process</processName>
        </config>
        <pd:inputBindings>
            <AssertionInput>
                <ns3:AssertType>
                    <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/TibUnit/ASSERT_EXCEPTION"/>
                </ns3:AssertType>
                <UniqueName>
                    <xsl:value-of select="concat(&#xA;BW:getCurrentProcessName($_processContext/ns1:ProcessContext/ProcessId),'-',&#xA;BW:getCurrentActivityName($_processContext/ns1:ProcessContext/ProcessId)&#xA;)"/>
                </UniqueName>
                <trueResult>
                    <messageCode>
                        <xsl:value-of select="'200'"/>
                    </messageCode>
                    <message>
                        <xsl:value-of select="'Expected Exception Occurred'"/>
                    </message>
                </trueResult>
                <falseResult>
                    <messageCode>
                        <xsl:value-of select="'500'"/>
                    </messageCode>
                    <message>
                        <xsl:value-of select="'failure'"/>
                    </message>
                </falseResult>
                <expectedResult/>
                <actualResult>
                    <value>
                        <xsl:value-of select="$dbentry/root/response"/>
                    </value>
                </actualResult>
                <xsl:copy-of select="$Start/root/TibUnit"/>
            </AssertionInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="dbentry">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>218</pd:x>
        <pd:y>59</pd:y>
        <config>
            <processName>/MyApp/DBEntry/dbentry.process</processName>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:transition>
        <pd:from>AssertDB</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>dbentry</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>dbentry</pd:from>
        <pd:to>AssertDB</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>