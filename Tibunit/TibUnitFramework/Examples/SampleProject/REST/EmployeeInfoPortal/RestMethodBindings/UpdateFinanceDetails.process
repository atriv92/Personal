<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns1="http://www.tibco.com/pe/EngineTypes" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://xmlns.tibco.com/bw/activity/xml/render/bytesEnvelope/2003/05" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx3="http://www.tibco.com/schemas/Assignment3/SharedResources/Schemas/Schema.xsd6" xmlns:pfx2="http://www.tibco.com/schemas/Assignment3/SharedResources/Schemas/Schema.xsd7" xmlns:pfx="http://www.tibco.com/schemas/Assignment3/SharedResources/Schemas/Schema.xsd2">
    <xsd:import namespace="http://www.tibco.com/schemas/Assignment3/SharedResources/Schemas/Schema.xsd2" schemaLocation="/SharedResources/Schemas/Finance.xsd"/>
    <xsd:import namespace="http://www.tibco.com/schemas/Assignment3/SharedResources/Schemas/Schema.xsd7" schemaLocation="/SharedResources/Schemas/SuccessOutput.xsd"/>
    <xsd:import namespace="http://www.tibco.com/schemas/Assignment3/SharedResources/Schemas/Schema.xsd6" schemaLocation="/SharedResources/Schemas/ErrorSchema.xsd"/>
    <pd:name>REST/EmployeeInfoPortal/RestMethodBindings/UpdateFinanceDetails.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:Finance"/>
    <pd:startX>150</pd:startX>
    <pd:startY>56</pd:startY>
    <pd:returnBindings>
        <xsl:choose>
            <xsl:when test="exists($_error/ns1:ErrorReport/MsgCode)">
                <Response>
                    <JsonString>
                        <xsl:value-of select="$Render-JSON-1/ns2:ActivityOutputClass/jsonString"/>
                    </JsonString>
                </Response>
            </xsl:when>
            <xsl:otherwise>
                <Response>
                    <JsonString>
                        <xsl:value-of select="$Render-JSON/ns2:ActivityOutputClass/jsonString"/>
                    </JsonString>
                </Response>
            </xsl:otherwise>
        </xsl:choose>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="Response">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="JsonString" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:endType>
    <pd:endX>645</pd:endX>
    <pd:endY>106</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1453379536678</pd:targetNamespace>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>255</pd:x>
        <pd:y>157</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="FTP Put">
        <pd:type>com.tibco.plugin.ftp.FTPPutActivity</pd:type>
        <pd:resourceType>ae.activities.FTPPutActivityUI</pd:resourceType>
        <pd:x>422</pd:x>
        <pd:y>56</pd:y>
        <config>
            <Timeout>120000</Timeout>
            <FireWall>false</FireWall>
            <isBinary>true</isBinary>
            <Append>false</Append>
            <useProcessData>true</useProcessData>
            <Overwrite>true</Overwrite>
            <SharedUserData>/Connections/FTP/FTP Connection.sharedftp</SharedUserData>
        </config>
        <pd:inputBindings>
            <FTPPutInputDataBinary>
                <RemoteFileName>
                    <xsl:value-of select="concat(&quot;/home/userftp/Test_AbhishekB/&quot;, concat($Start/pfx:Finance/pfx:KBGID,&quot;.txt&quot;))"/>
                </RemoteFileName>
                <Data>
                    <xsl:value-of select="$Render-XML/xmlBytes"/>
                </Data>
            </FTPPutInputDataBinary>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Map Data">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>390</pd:x>
        <pd:y>157</pd:y>
        <config>
            <element ref="pfx3:Error"/>
        </config>
        <pd:inputBindings>
            <xsl:choose>
                <xsl:when test="exists($_error_Render-XML/ActivityErrors)">
                    <pfx3:Error>
                        <pfx3:ErrorStatus>
                            <xsl:value-of select="&quot;Error in Rendering XML&quot;"/>
                        </pfx3:ErrorStatus>
                        <pfx3:ErrorCode>
                            <xsl:value-of select="$_error/ns1:ErrorReport/MsgCode"/>
                        </pfx3:ErrorCode>
                    </pfx3:Error>
                </xsl:when>
                <xsl:when test="exists($_error_FTP-Put/ActivityErrors)">
                    <pfx3:Error>
                        <pfx3:ErrorStatus>
                            <xsl:value-of select="&quot;Error in Updating Financial Details&quot;"/>
                        </pfx3:ErrorStatus>
                        <pfx3:ErrorCode>
                            <xsl:value-of select="$_error/ns1:ErrorReport/MsgCode"/>
                        </pfx3:ErrorCode>
                    </pfx3:Error>
                </xsl:when>
                <xsl:otherwise>
                    <pfx3:Error>
                        <pfx3:ErrorStatus>
                            <xsl:value-of select="$_error/ns1:ErrorReport/Msg"/>
                        </pfx3:ErrorStatus>
                        <pfx3:ErrorCode>
                            <xsl:value-of select="$_error/ns1:ErrorReport/MsgCode"/>
                        </pfx3:ErrorCode>
                    </pfx3:Error>
                </xsl:otherwise>
            </xsl:choose>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Render JSON">
        <pd:type>com.tibco.plugin.json.activities.JSONRenderActivity</pd:type>
        <pd:resourceType>ae.activities.JSONRenderActivity</pd:resourceType>
        <pd:x>528</pd:x>
        <pd:y>56</pd:y>
        <config>
            <SchemaType>xsdType</SchemaType>
            <BadgerfishCheck>false</BadgerfishCheck>
            <ActivityInputEditor ref="pfx2:OperationResponse"/>
        </config>
        <pd:inputBindings>
            <ns2:ActivityInputClass>
                <pfx2:OperationResponse>
                    <pfx2:StatusCode>
                        <xsl:value-of select="&quot;Success&quot;"/>
                    </pfx2:StatusCode>
                    <pfx2:StatusMessage>
                        <xsl:value-of select="&quot;Employee's Financial Details are Updated Successfully&quot;"/>
                    </pfx2:StatusMessage>
                </pfx2:OperationResponse>
            </ns2:ActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Render XML">
        <pd:type>com.tibco.plugin.xml.XMLRendererActivity</pd:type>
        <pd:resourceType>ae.activities.XMLRendererActivity</pd:resourceType>
        <pd:x>300</pd:x>
        <pd:y>56</pd:y>
        <config>
            <renderAsText>binary</renderAsText>
            <validateInput>true</validateInput>
            <term ref="pfx:Finance"/>
        </config>
        <pd:inputBindings>
            <ns:xmlInput>
                <pfx:Finance>
                    <xsl:if test="$Start/pfx:Finance/pfx:KBGID">
                        <pfx:KBGID>
                            <xsl:value-of select="$Start/pfx:Finance/pfx:KBGID"/>
                        </pfx:KBGID>
                    </xsl:if>
                    <xsl:if test="$Start/pfx:Finance/pfx:FullName">
                        <pfx:FullName>
                            <xsl:value-of select="$Start/pfx:Finance/pfx:FullName"/>
                        </pfx:FullName>
                    </xsl:if>
                    <xsl:if test="$Start/pfx:Finance/pfx:PAN">
                        <pfx:PAN>
                            <xsl:value-of select="$Start/pfx:Finance/pfx:PAN"/>
                        </pfx:PAN>
                    </xsl:if>
                    <xsl:if test="$Start/pfx:Finance/pfx:Payband">
                        <pfx:Payband>
                            <xsl:value-of select="$Start/pfx:Finance/pfx:Payband"/>
                        </pfx:Payband>
                    </xsl:if>
                    <xsl:if test="$Start/pfx:Finance/pfx:Salary">
                        <pfx:Salary>
                            <xsl:value-of select="$Start/pfx:Finance/pfx:Salary"/>
                        </pfx:Salary>
                    </xsl:if>
                    <xsl:if test="$Start/pfx:Finance/pfx:Allowances">
                        <pfx:Allowances>
                            <xsl:value-of select="$Start/pfx:Finance/pfx:Allowances"/>
                        </pfx:Allowances>
                    </xsl:if>
                    <xsl:if test="$Start/pfx:Finance/pfx:Retirals">
                        <pfx:Retirals>
                            <xsl:value-of select="$Start/pfx:Finance/pfx:Retirals"/>
                        </pfx:Retirals>
                    </xsl:if>
                    <xsl:if test="$Start/pfx:Finance/pfx:Bonus">
                        <pfx:Bonus>
                            <xsl:value-of select="$Start/pfx:Finance/pfx:Bonus"/>
                        </pfx:Bonus>
                    </xsl:if>
                </pfx:Finance>
            </ns:xmlInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Render-JSON-1">
        <pd:type>com.tibco.plugin.json.activities.JSONRenderActivity</pd:type>
        <pd:resourceType>ae.activities.JSONRenderActivity</pd:resourceType>
        <pd:x>528</pd:x>
        <pd:y>157</pd:y>
        <config>
            <SchemaType>xsdType</SchemaType>
            <BadgerfishCheck>false</BadgerfishCheck>
            <ActivityInputEditor ref="pfx3:Error"/>
        </config>
        <pd:inputBindings>
            <ns2:ActivityInputClass>
                <xsl:copy-of select="$Map-Data/pfx3:Error"/>
            </ns2:ActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Render XML</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>FTP Put</pd:from>
        <pd:to>Render JSON</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Render XML</pd:from>
        <pd:to>FTP Put</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>Map Data</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Map Data</pd:from>
        <pd:to>Render-JSON-1</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Render-JSON-1</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Render JSON</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>