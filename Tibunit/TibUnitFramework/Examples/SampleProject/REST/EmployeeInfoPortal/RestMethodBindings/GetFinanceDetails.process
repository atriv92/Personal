<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns1="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/pe/EngineTypes" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx2="http://www.tibco.com/schemas/Assignment3/SharedResources/Schemas/Schema.xsd6" xmlns:pfx="http://www.tibco.com/schemas/Assignment3/SharedResources/Schemas/Schema.xsd2">
    <xsd:import namespace="http://www.tibco.com/schemas/Assignment3/SharedResources/Schemas/Schema.xsd2" schemaLocation="/SharedResources/Schemas/Finance.xsd"/>
    <xsd:import namespace="http://www.tibco.com/schemas/Assignment3/SharedResources/Schemas/Schema.xsd6" schemaLocation="/SharedResources/Schemas/ErrorSchema.xsd"/>
    <pd:name>REST/EmployeeInfoPortal/RestMethodBindings/GetFinanceDetails.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="KBGID" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>150</pd:startX>
    <pd:startY>62</pd:startY>
    <pd:returnBindings>
        <xsl:choose>
            <xsl:when test="exists($_error/ns:ErrorReport/MsgCode)">
                <Response>
                    <JsonString>
                        <xsl:value-of select="$Render-JSON-1/ns1:ActivityOutputClass/jsonString"/>
                    </JsonString>
                </Response>
            </xsl:when>
            <xsl:otherwise>
                <Response>
                    <JsonString>
                        <xsl:value-of select="$Render-JSON/ns1:ActivityOutputClass/jsonString"/>
                    </JsonString>
                </Response>
            </xsl:otherwise>
        </xsl:choose>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="Response">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="JsonString" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:endType>
    <pd:endX>663</pd:endX>
    <pd:endY>117</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1453379536678</pd:targetNamespace>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>254</pd:x>
        <pd:y>160</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="FTP Get">
        <pd:type>com.tibco.plugin.ftp.FTPGetActivity</pd:type>
        <pd:resourceType>ae.activities.FTPGetActivityUI</pd:resourceType>
        <pd:x>274</pd:x>
        <pd:y>62</pd:y>
        <config>
            <Timeout>120000</Timeout>
            <isBinary>true</isBinary>
            <Overwrite>false</Overwrite>
            <useProcessData>true</useProcessData>
            <SharedUserData>/Connections/FTP/FTP Connection.sharedftp</SharedUserData>
        </config>
        <pd:inputBindings>
            <FTPGetInputData>
                <RemoteFileName>
                    <xsl:value-of select="concat(&quot;/home/userftp/Test_AbhishekB/&quot;, concat($Start/root/KBGID,&quot;.txt&quot;))"/>
                </RemoteFileName>
            </FTPGetInputData>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Map Data">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>369</pd:x>
        <pd:y>160</pd:y>
        <config>
            <element ref="pfx2:Error"/>
        </config>
        <pd:inputBindings>
            <xsl:choose>
                <xsl:when test="exists($_error_FTP-Get/ActivityErrors)">
                    <pfx2:Error>
                        <pfx2:ErrorStatus>
                            <xsl:value-of select="&quot;Error in Fetching the Finance Details&quot;"/>
                        </pfx2:ErrorStatus>
                        <pfx2:ErrorCode>
                            <xsl:value-of select="$_error/ns:ErrorReport/MsgCode"/>
                        </pfx2:ErrorCode>
                    </pfx2:Error>
                </xsl:when>
                <xsl:when test="exists($_error_Parse-XML/ActivityErrors)">
                    <pfx2:Error>
                        <pfx2:ErrorStatus>
                            <xsl:value-of select="&quot;Error in Parsing to XML&quot;"/>
                        </pfx2:ErrorStatus>
                        <pfx2:ErrorCode>
                            <xsl:value-of select="$_error/ns:ErrorReport/MsgCode"/>
                        </pfx2:ErrorCode>
                    </pfx2:Error>
                </xsl:when>
                <xsl:otherwise>
                    <pfx2:Error>
                        <pfx2:ErrorStatus>
                            <xsl:value-of select="$_error/ns:ErrorReport/Msg"/>
                        </pfx2:ErrorStatus>
                        <pfx2:ErrorCode>
                            <xsl:value-of select="$_error/ns:ErrorReport/MsgCode"/>
                        </pfx2:ErrorCode>
                    </pfx2:Error>
                </xsl:otherwise>
            </xsl:choose>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Parse XML">
        <pd:type>com.tibco.plugin.xml.XMLParseActivity</pd:type>
        <pd:resourceType>ae.activities.XMLParseActivity</pd:resourceType>
        <pd:x>374</pd:x>
        <pd:y>62</pd:y>
        <config>
            <inputStyle>binary</inputStyle>
            <xsdVersion>default</xsdVersion>
            <validateOutput>true</validateOutput>
            <term ref="pfx:Finance"/>
        </config>
        <pd:inputBindings>
            <xmlBinary>
                <bytes>
                    <xsl:value-of select="$FTP-Get/FTPGetOutputBinary/BinaryData"/>
                </bytes>
            </xmlBinary>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Render JSON">
        <pd:type>com.tibco.plugin.json.activities.JSONRenderActivity</pd:type>
        <pd:resourceType>ae.activities.JSONRenderActivity</pd:resourceType>
        <pd:x>526</pd:x>
        <pd:y>62</pd:y>
        <config>
            <SchemaType>xsdType</SchemaType>
            <BadgerfishCheck>false</BadgerfishCheck>
            <ActivityInputEditor ref="pfx:Finance"/>
        </config>
        <pd:inputBindings>
            <ns1:ActivityInputClass>
                <xsl:copy-of select="$Parse-XML/pfx:Finance"/>
            </ns1:ActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Render-JSON-1">
        <pd:type>com.tibco.plugin.json.activities.JSONRenderActivity</pd:type>
        <pd:resourceType>ae.activities.JSONRenderActivity</pd:resourceType>
        <pd:x>513</pd:x>
        <pd:y>160</pd:y>
        <config>
            <SchemaType>xsdType</SchemaType>
            <BadgerfishCheck>false</BadgerfishCheck>
            <ActivityInputEditor ref="pfx2:Error"/>
        </config>
        <pd:inputBindings>
            <ns1:ActivityInputClass>
                <xsl:copy-of select="$Map-Data/pfx2:Error"/>
            </ns1:ActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>FTP Get</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>FTP Get</pd:from>
        <pd:to>Parse XML</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Parse XML</pd:from>
        <pd:to>Render JSON</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>Map Data</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Map Data</pd:from>
        <pd:to>Render-JSON-1</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Render-JSON-1</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Render JSON</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>