<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns1="http://www.tibco.com/pe/EngineTypes" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx3="http://www.tibco.com/schemas/Assignment3/SharedResources/Schemas/Schema.xsd6" xmlns:pfx2="http://www.tibco.com/schemas/Assignment3/SharedResources/Schemas/Schema.xsd7" xmlns:pfx="http://www.tibco.com/schemas/Assignment3/SharedResources/Schemas/Schema.xsd">
    <xsd:import namespace="http://www.tibco.com/schemas/Assignment3/SharedResources/Schemas/Schema.xsd" schemaLocation="/SharedResources/Schemas/ProjectAssignment.xsd"/>
    <xsd:import namespace="http://www.tibco.com/schemas/Assignment3/SharedResources/Schemas/Schema.xsd7" schemaLocation="/SharedResources/Schemas/SuccessOutput.xsd"/>
    <xsd:import namespace="http://www.tibco.com/schemas/Assignment3/SharedResources/Schemas/Schema.xsd6" schemaLocation="/SharedResources/Schemas/ErrorSchema.xsd"/>
    <pd:name>REST/EmployeeInfoPortal/RestMethodBindings/UpdateProjectAssignment.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:ProjectAssignment"/>
    <pd:startX>86</pd:startX>
    <pd:startY>80</pd:startY>
    <pd:returnBindings>
        <xsl:choose>
            <xsl:when test="exists($_error/ns1:ErrorReport/MsgCode)">
                <Response>
                    <JsonString>
                        <xsl:value-of select="$Render-JSON-1/ns:ActivityOutputClass/jsonString"/>
                    </JsonString>
                </Response>
            </xsl:when>
            <xsl:otherwise>
                <Response>
                    <JsonString>
                        <xsl:value-of select="$Render-JSON/ns:ActivityOutputClass/jsonString"/>
                    </JsonString>
                </Response>
            </xsl:otherwise>
        </xsl:choose>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="Response">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="JsonString" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:endType>
    <pd:endX>470</pd:endX>
    <pd:endY>122</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1453379536678</pd:targetNamespace>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>105</pd:x>
        <pd:y>170</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="JDBC Update">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>218</pd:x>
        <pd:y>80</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/Connections/JDBC/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <statement>UPDATE PROJECTASSIGNMENT SET BILLABILITY=?,ROLE=?,DESIGNATION=?,PID=?,PROJECTNAME=?,PIDSTARTDATE=?,PIDENDDATE=? WHERE ORACLEID=?</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>BILLABILITY</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>ROLE</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>DESIGNATION</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>PID</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>PROJECTNAME</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>PIDSTARTDATE</parameterName>
                    <dataType>DATE</dataType>
                </parameter>
                <parameter>
                    <parameterName>PIDENDDATE</parameterName>
                    <dataType>DATE</dataType>
                </parameter>
                <parameter>
                    <parameterName>ORACLEID</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <BILLABILITY>
                    <xsl:value-of select="$Start/pfx:ProjectAssignment/pfx:Bllability"/>
                </BILLABILITY>
                <ROLE>
                    <xsl:value-of select="$Start/pfx:ProjectAssignment/pfx:Role"/>
                </ROLE>
                <DESIGNATION>
                    <xsl:value-of select="$Start/pfx:ProjectAssignment/pfx:Designation"/>
                </DESIGNATION>
                <PID>
                    <xsl:value-of select="$Start/pfx:ProjectAssignment/pfx:PID"/>
                </PID>
                <PROJECTNAME>
                    <xsl:value-of select="$Start/pfx:ProjectAssignment/pfx:ProjectName"/>
                </PROJECTNAME>
                <PIDSTARTDATE>
                    <xsl:value-of select="$Start/pfx:ProjectAssignment/pfx:PIDStartDate"/>
                </PIDSTARTDATE>
                <PIDENDDATE>
                    <xsl:value-of select="$Start/pfx:ProjectAssignment/pfx:PIDEndDate"/>
                </PIDENDDATE>
                <ORACLEID>
                    <xsl:value-of select="$Start/pfx:ProjectAssignment/pfx:OracleID"/>
                </ORACLEID>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Map Data">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>224</pd:x>
        <pd:y>170</pd:y>
        <config>
            <element ref="pfx3:Error"/>
        </config>
        <pd:inputBindings>
            <xsl:choose>
                <xsl:when test="exists($_error_JDBC-Update/ActivityErrors)">
                    <pfx3:Error>
                        <pfx3:ErrorStatus>
                            <xsl:value-of select="&quot;Error in Updating the Project Assignment Details&quot;"/>
                        </pfx3:ErrorStatus>
                        <pfx3:ErrorCode>
                            <xsl:value-of select="$_error/ns1:ErrorReport/MsgCode"/>
                        </pfx3:ErrorCode>
                    </pfx3:Error>
                </xsl:when>
                <xsl:otherwise>
                    <pfx3:Error>
                        <pfx3:ErrorStatus>
                            <xsl:value-of select="$_error/ns1:ErrorReport/Msg"/>
                        </pfx3:ErrorStatus>
                        <pfx3:ErrorCode>
                            <xsl:value-of select="$_error/ns1:ErrorReport/MsgCode"/>
                        </pfx3:ErrorCode>
                    </pfx3:Error>
                </xsl:otherwise>
            </xsl:choose>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Render JSON">
        <pd:type>com.tibco.plugin.json.activities.JSONRenderActivity</pd:type>
        <pd:resourceType>ae.activities.JSONRenderActivity</pd:resourceType>
        <pd:x>370</pd:x>
        <pd:y>80</pd:y>
        <config>
            <SchemaType>xsdType</SchemaType>
            <BadgerfishCheck>false</BadgerfishCheck>
            <ActivityInputEditor ref="pfx2:OperationResponse"/>
        </config>
        <pd:inputBindings>
            <ns:ActivityInputClass>
                <pfx2:OperationResponse>
                    <pfx2:StatusCode>
                        <xsl:value-of select="&quot;Success&quot;"/>
                    </pfx2:StatusCode>
                    <pfx2:StatusMessage>
                        <xsl:value-of select="&quot;Employee's Project Assignment Details Updated Successfully&quot;"/>
                    </pfx2:StatusMessage>
                </pfx2:OperationResponse>
            </ns:ActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Render-JSON-1">
        <pd:type>com.tibco.plugin.json.activities.JSONRenderActivity</pd:type>
        <pd:resourceType>ae.activities.JSONRenderActivity</pd:resourceType>
        <pd:x>356</pd:x>
        <pd:y>170</pd:y>
        <config>
            <SchemaType>xsdType</SchemaType>
            <BadgerfishCheck>false</BadgerfishCheck>
            <ActivityInputEditor ref="pfx3:Error"/>
        </config>
        <pd:inputBindings>
            <ns:ActivityInputClass>
                <pfx3:Error>
                    <pfx3:ErrorStatus>
                        <xsl:value-of select="$Map-Data/pfx3:Error/pfx3:ErrorStatus"/>
                    </pfx3:ErrorStatus>
                    <pfx3:ErrorCode>
                        <xsl:value-of select="$Map-Data/pfx3:Error/pfx3:ErrorCode"/>
                    </pfx3:ErrorCode>
                </pfx3:Error>
            </ns:ActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>JDBC Update</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>JDBC Update</pd:from>
        <pd:to>Render JSON</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>Map Data</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Map Data</pd:from>
        <pd:to>Render-JSON-1</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Render-JSON-1</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Render JSON</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>