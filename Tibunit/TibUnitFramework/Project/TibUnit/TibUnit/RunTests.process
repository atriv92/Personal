<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns2="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns1="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:ns3="http://www.tibco.com/pe/EngineTypes" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx4="http://www.tibco.com/schemas/TibUnit/Schema.xsd" xmlns:pfx="com/tibco/pe/commands">
    <xsd:import namespace="http://www.tibco.com/schemas/TibUnit/Schema.xsd" schemaLocation="/TibUnit/Schema.xsd"/>
    <pd:name>TibUnit/RunTests.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startX>51</pd:startX>
    <pd:startY>64</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>728</pd:endX>
    <pd:endY>266</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1451902982444</pd:targetNamespace>
    <pd:group name="LoopTibUnitTests">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>60</pd:x>
        <pd:y>141</pd:y>
        <pd:width>563</pd:width>
        <pd:height>240</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>inputLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:over>$GetTestsToRun/TibUnits/TibUnit</pd:over>
            <pd:iterationElementSlot>tibUnit</pd:iterationElementSlot>
            <pd:indexSlot>p</pd:indexSlot>
            <pd:activityOutputName/>
            <pd:accumulateOutput>false</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>CallUnitTest</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>SetVariable</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>CallUnitTest</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Catch-1</pd:from>
            <pd:to>Log-2</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>GetVariable</pd:from>
            <pd:to>Map-Result</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Map-Result</pd:from>
            <pd:to>SetVariable</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Log-2</pd:from>
            <pd:to>GetVariable</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="CallUnitTest">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>339</pd:x>
            <pd:y>204</pd:y>
            <config>
                <processName>/TibUnit/CallUnitTest.process</processName>
                <processNameXPath/>
            </config>
            <pd:inputBindings>
                <root>
                    <processFileName>
                        <xsl:value-of select="$tibUnit/TibUnit/processFileName"/>
                    </processFileName>
                    <parentFolder>
                        <xsl:value-of select="$tibUnit/TibUnit/parentFolder"/>
                    </parentFolder>
                    <completeProcessName>
                        <xsl:value-of select="$tibUnit/TibUnit/completeProcessName"/>
                    </completeProcessName>
                </root>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Catch-1">
            <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
            <pd:resourceType>ae.activities.catch</pd:resourceType>
            <pd:x>126</pd:x>
            <pd:y>316</pd:y>
            <pd:handler>true</pd:handler>
            <config>
                <catchAll>true</catchAll>
            </config>
            <pd:inputBindings/>
        </pd:activity>
        <pd:activity name="GetVariable">
            <pd:type>com.tibco.pe.core.GetSharedVariableActivity</pd:type>
            <pd:resourceType>ae.activities.getSharedVariable</pd:resourceType>
            <pd:x>322</pd:x>
            <pd:y>314</pd:y>
            <config>
                <variableConfig>/TibUnit/ResultXML.sharedvariable</variableConfig>
            </config>
            <pd:inputBindings/>
        </pd:activity>
        <pd:activity name="Map-Result">
            <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
            <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
            <pd:x>432</pd:x>
            <pd:y>315</pd:y>
            <config>
                <element ref="pfx4:TibUnitTest"/>
            </config>
            <pd:inputBindings>
                <pfx4:TibUnitTest>
                    <pfx4:processId>
                        <xsl:value-of select="$_processContext/ns3:ProcessContext/ProcessId"/>
                    </pfx4:processId>
                    <pfx4:category>
                        <xsl:value-of select="$tibUnit/TibUnit/parentFolder"/>
                    </pfx4:category>
                    <pfx4:name>
                        <xsl:value-of select="$tibUnit/TibUnit/processFileName"/>
                    </pfx4:name>
                    <pfx4:result>
                        <xsl:value-of select="false()"/>
                    </pfx4:result>
                    <pfx4:timestamp>
                        <xsl:value-of select="current-dateTime()"/>
                    </pfx4:timestamp>
                    <pfx4:messageCode>
                        <xsl:value-of select="'--ERROR--'"/>
                    </pfx4:messageCode>
                    <xsl:if test="$_error/ns3:ErrorReport/Msg">
                        <pfx4:message>
                            <xsl:value-of select="$_error/ns3:ErrorReport/Msg"/>
                        </pfx4:message>
                    </xsl:if>
                </pfx4:TibUnitTest>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="SetVariable">
            <pd:type>com.tibco.pe.core.SetSharedVariableActivity</pd:type>
            <pd:resourceType>ae.activities.setSharedVariable</pd:resourceType>
            <pd:x>524</pd:x>
            <pd:y>318</pd:y>
            <config>
                <variableConfig>/TibUnit/ResultXML.sharedvariable</variableConfig>
                <showResult>true</showResult>
            </config>
            <pd:inputBindings>
                <pfx4:TibUnitTestResult>
                    <xsl:attribute xmlns:xsl="http://www.w3.org/1999/XSL/Transform" name="engineName">
                        <xsl:value-of select="$GetVariable/pfx4:TibUnitTestResult/@engineName"/>
                    </xsl:attribute>
                    <xsl:attribute xmlns:xsl="http://www.w3.org/1999/XSL/Transform" name="startTime">
                        <xsl:value-of select="$GetVariable/pfx4:TibUnitTestResult/@startTime"/>
                    </xsl:attribute>
                    <xsl:if test="$GetVariable/pfx4:TibUnitTestResult/@endTime">
                        <xsl:attribute name="endTime">
                            <xsl:value-of select="$GetVariable/pfx4:TibUnitTestResult/@endTime"/>
                        </xsl:attribute>
                    </xsl:if>
                    <xsl:attribute name="passCount">
                        <xsl:value-of select="$GetVariable/pfx4:TibUnitTestResult/@passCount"/>
                    </xsl:attribute>
                    <xsl:attribute name="failCount">
                        <xsl:value-of select="$GetVariable/pfx4:TibUnitTestResult/@failCount + 1"/>
                    </xsl:attribute>
                    <xsl:copy-of select="$GetVariable/pfx4:TibUnitTestResult/pfx4:TibUnitTest"/>
                    <xsl:copy-of select="$Map-Result/pfx4:TibUnitTest"/>
                </pfx4:TibUnitTestResult>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Log-2">
            <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
            <pd:resourceType>ae.activities.log</pd:resourceType>
            <pd:x>215</pd:x>
            <pd:y>316</pd:y>
            <config>
                <role>Error</role>
            </config>
            <pd:inputBindings>
                <ns2:ActivityInput>
                    <message>
                        <xsl:value-of select="tib:render-xml($_error/ns3:ErrorReport)"/>
                    </message>
                    <msgCode>
                        <xsl:value-of select="$tibUnit/TibUnit/completeProcessName"/>
                    </msgCode>
                </ns2:ActivityInput>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="Log">
        <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
        <pd:resourceType>ae.activities.log</pd:resourceType>
        <pd:x>176</pd:x>
        <pd:y>64</pd:y>
        <config>
            <role>User</role>
        </config>
        <pd:inputBindings>
            <ns2:ActivityInput>
                <message>
                    <xsl:value-of select="'TestSuite:Started'"/>
                </message>
            </ns2:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Log-1">
        <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
        <pd:resourceType>ae.activities.log</pd:resourceType>
        <pd:x>517</pd:x>
        <pd:y>67</pd:y>
        <config>
            <role>User</role>
        </config>
        <pd:inputBindings>
            <ns2:ActivityInput>
                <message>
                    <xsl:value-of select="concat('Number of TibUnitTestsFound-', count($GetTestsToRun/TibUnits/TibUnit))"/>
                </message>
            </ns2:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="GetTestsToRun">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>341</pd:x>
        <pd:y>67</pd:y>
        <config>
            <processName>/TibUnit/GetTestsToRun.process</processName>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:transition>
        <pd:from>LoopTibUnitTests</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log-1</pd:from>
        <pd:to>LoopTibUnitTests</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Log</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log</pd:from>
        <pd:to>GetTestsToRun</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>GetTestsToRun</pd:from>
        <pd:to>Log-1</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>